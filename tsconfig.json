{
  "compilerOptions": {
    "target": "es6",
    "lib": ["es6", "dom"],
    "types": ["cypress"],
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "baseUrl": "./",
    "paths": {
      "@pages/*": ["cypress/support/pages/*"]
    }
  },
  "include": [
    "cypress/**/*.ts",
    "cypress.config.ts"
  ]
}
